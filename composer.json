{
    "name": "padosoft/laravel-translation-manager",
    "description": "Manage Laravel Translations",
    "keywords": ["laravel", "translations", "translator"],
    "license": "MIT",
    "authors": [
        {
            "name": "padosoft",
            "email": "helpdesk@padosoft.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.x|6.x|7.x",
        "illuminate/translation": "5.x|6.x|7.x",
        "symfony/finder": "~2.3|~3.0|~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Barryvdh\\TranslationManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Barryvdh\\TranslationManager\\ManagerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
